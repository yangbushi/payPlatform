log4j.rootLogger=INFO,stdout,logfile

log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=%d %p [%c] - %m%n


log4j.appender.logfile=org.apache.log4j.RollingFileAppender
log4j.appender.logfile.File=D:/pay/hr-hos-pay.log
log4j.appender.logfile.MaxFileSize=1024KB
log4j.appender.logfile.MaxBackupIndex=100
log4j.appender.logfile.layout=org.apache.log4j.PatternLayout
log4j.appender.logfile.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} [%t] [%c] [%p] - %m%n


log4j.logger.HisTimeLog=INFO,timeLog1
log4j.appender.timeLog1=org.apache.log4j.RollingFileAppender
log4j.appender.timeLog1.MaxFileSize=1024KB
log4j.appender.timeLog1.MaxBackupIndex=100
log4j.appender.timeLog1.File=D:/pay/hr-hos-pay.log
log4j.appender.timeLog1.layout=org.apache.log4j.PatternLayout
log4j.appender.timeLog1.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} [%t] [%c] [%p] - %m%n
log4j.additivity.HisTimeLog=false

log4j.logger.CloudTimeLog=INFO,timeLog2
log4j.appender.timeLog2=org.apache.log4j.RollingFileAppender
log4j.appender.timeLog2.MaxFileSize=1024KB
log4j.appender.timeLog2.MaxBackupIndex=100
log4j.appender.timeLog2.File=D:/pay/hr-hos-pay.log
log4j.appender.timeLog2.layout=org.apache.log4j.PatternLayout
log4j.appender.timeLog2.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} [%t] [%c] [%p] - %m%n
log4j.additivity.CloudTimeLog=false

log4j.logger.refoundLog=INFO,refoundLog2
log4j.appender.refoundLog2=org.apache.log4j.RollingFileAppender
log4j.appender.refoundLog2.MaxFileSize=1024KB
log4j.appender.refoundLog2.MaxBackupIndex=100
log4j.appender.refoundLog2.File=D:/pay/hr-hos-pay.log
log4j.appender.refoundLog2.layout=org.apache.log4j.PatternLayout
log4j.appender.refoundLog2.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} [%t] [%c] [%p] - %m%n
log4j.additivity.refoundLog=false

