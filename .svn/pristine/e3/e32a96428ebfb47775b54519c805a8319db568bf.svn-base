package com.defray.bean;

public class AlipayInfoBean implements java.io.Serializable {

	private static final long serialVersionUID = -2540017060686870793L;
	private String subject;
	private String outTradeNo;
	private String totalFee;
	
	private String notifyUrl;
	private String abnormalUrl;
	
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	public String getOutTradeNo() {
		return outTradeNo;
	}
	public void setOutTradeNo(String outTradeNo) {
		this.outTradeNo = outTradeNo;
	}
	public String getTotalFee() {
		return totalFee;
	}
	public void setTotalFee(String totalFee) {
		this.totalFee = totalFee;
	}
	public String getNotifyUrl() {
		return notifyUrl;
	}
	public void setNotifyUrl(String notifyUrl) {
		this.notifyUrl = notifyUrl;
	}
	public String getAbnormalUrl() {
		return abnormalUrl;
	}
	public void setAbnormalUrl(String abnormalUrl) {
		this.abnormalUrl = abnormalUrl;
	}
	
	
}
